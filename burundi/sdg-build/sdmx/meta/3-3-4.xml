        <gen:ReportedAttribute id="SDG_GOAL">
          
          <com:Text xml:lang="en"><![CDATA[Goal 3. Ensure healthy lives and promote well-being for all at all ages]]></com:Text>
          
          <com:Text xml:lang="fr"><![CDATA[Objectif 3. Permettre à tous de vivre en bonne santé et promouvoir le bien-être de tous à tout âge]]></com:Text>
          
        </gen:ReportedAttribute>
        
        <gen:ReportedAttribute id="SDG_TARGET">
          
          <com:Text xml:lang="en"><![CDATA[3.3 By 2030, end the epidemic of AIDS, tuberculosis, malaria and neglected tropical diseases and combat hepatitis, water-borne diseases and other communicable diseases]]></com:Text>
          
          <com:Text xml:lang="fr"><![CDATA[3.3 D’ici à 2030, mettre fin à l’épidémie de sida, à la tuberculose, au paludisme et aux maladies tropicales négligées et combattre l’hépatite, les maladies transmises par l’eau et autres maladies transmissibles]]></com:Text>
          
        </gen:ReportedAttribute>
        
        <gen:ReportedAttribute id="SDG_INDICATOR">
          
          <com:Text xml:lang="en"><![CDATA[3.3.4. Malaria prevalence rate <5 years]]></com:Text>
          
          <com:Text xml:lang="fr"><![CDATA[3.3.4. Taux de prévalence du paludisme < 5ans]]></com:Text>
          
        </gen:ReportedAttribute>
        
