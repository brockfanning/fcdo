        <gen:ReportedAttribute id="STAT_CONC_DEF">
          
          <com:Text xml:lang="en"><![CDATA[The prevalence of malaria is the number of cases of malaria for children under 5 out of the total number of children under 5.]]></com:Text>
          
          <com:Text xml:lang="fr"><![CDATA[La prévalence du paludisme est le nombre de cas de paludisme pour les enfants de moins de 5 ans sur le nombre total des enfants de moins de 5ans.]]></com:Text>
          
        </gen:ReportedAttribute>
        
        <gen:ReportedAttribute id="UNIT_MEASURE">
          
