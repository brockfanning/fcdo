        <gen:ReportedAttribute id="FREQ_COLL">
          
          <com:Text xml:lang="en"><![CDATA[<p>Annual</p>]]></com:Text>
          
        </gen:ReportedAttribute>
        
        <gen:ReportedAttribute id="DATA_SOURCE">
          
          <com:Text xml:lang="en"><![CDATA[<p>Reports of <a href="http://www.minijust.gov.rw/fileadmin/Documents/JRLOS_Documents/SSP_II_2013-2018.pdf">Justice, Reconciliation, Law &amp; Order</a> (JRLOS)</p>]]></com:Text>
          
        </gen:ReportedAttribute>
        
        <gen:ReportedAttribute id="DATA_COMP">
          
          <com:Text xml:lang="en"><![CDATA[<p>The total number of unsentenced persons held in detention divided by the total number of persons held in detention, on a specified date.</p>]]></com:Text>
          
        </gen:ReportedAttribute>
        
        <gen:ReportedAttribute id="COVERAGE">
          
          <com:Text xml:lang="en"><![CDATA[<h2>Geographical coverage</h2><p>National</p><h2>Disaggregation</h2><p>- age </p><p>- sex </p><p>- length of pre-trial (unsentenced) detention</p>]]></com:Text>
          
