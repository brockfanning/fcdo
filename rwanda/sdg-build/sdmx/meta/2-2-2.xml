  <mes:MetadataSet structureRef="MDS1" setID="f77c1456-f85a-4f83-b372-fe7bee28a853">
    <com:Name xml:lang="en">SDG METADATA SET</com:Name>
    <gen:Report id="SDG_META_RPT">
      <gen:Target id="KEY_TARGET">
        <gen:ReferenceValue id="DIMENSION_DESCRIPTOR_VALUES_TARGET">
          <gen:DataKey>
            <com:KeyValue id="SERIES">
              <com:Value>SH_STA_WAST</com:Value>
            </com:KeyValue>
            <com:KeyValue id="REPORTING_TYPE">
              <com:Value>N</com:Value>
            </com:KeyValue>
            <com:KeyValue id="REF_AREA">
              <com:Value>RW</com:Value>
            </com:KeyValue>
          </gen:DataKey>
        </gen:ReferenceValue>
        <gen:ReferenceValue id="Dataflow">
          <gen:ObjectReference>
            <URN>urn:sdmx:org.sdmx.infomodel.datastructure.Dataflow=IAEG-SDGs:DF_SDG_GLC(1.0)</URN>
          </gen:ObjectReference>
        </gen:ReferenceValue>
      </gen:Target>
      <gen:AttributeSet>
        
        <gen:ReportedAttribute id="SDG_TARGET">
          
          <com:Text xml:lang="en"><![CDATA[<p>2</p><p>By 2030, end all forms of malnutrition, including achieving, by 2025, the internationally agreed targets on stunting and wasting in children under 5 years of age, and address the nutritional needs of adolescent girls, pregnant and lactating women and older persons</p>]]></com:Text>
          
        </gen:ReportedAttribute>
        
        <gen:ReportedAttribute id="SDG_INDICATOR">
          
          <com:Text xml:lang="en"><![CDATA[<p>2.2.2</p><p>Prevalence of malnutrition (weight for height &gt;+2 or &lt;-2 standard deviation from the median of the WHO Child Growth Standards) among children under 5 years of age, by type (wasting and overweight)</p>]]></com:Text>
          
        </gen:ReportedAttribute>
        
        <gen:ReportedAttribute id="META_LAST_UPDATE">
          
          <com:Text xml:lang="en"><![CDATA[<p>2014-15</p>]]></com:Text>
          
        </gen:ReportedAttribute>
        
        <gen:ReportedAttribute id="SDG_CUSTODIAN_AGENCIES">
          
          <com:Text xml:lang="en"><![CDATA[<p>UNICEF</p>]]></com:Text>
          
        </gen:ReportedAttribute>
        
        <gen:ReportedAttribute id="CONTACT_ORGANISATION">
          
          <com:Text xml:lang="en"><![CDATA[<p>National Institute of Statistics of Rwanda</p>]]></com:Text>
          
        </gen:ReportedAttribute>
        
        <gen:ReportedAttribute id="CONTACT_EMAIL">
          
          <com:Text xml:lang="en"><![CDATA[<p><a href="mailto:info@statistics.gov.rw">info@statistics.gov.rw</a></p>]]></com:Text>
          
        </gen:ReportedAttribute>
        
        <gen:ReportedAttribute id="STAT_CONC_DEF">
          
          <com:Text xml:lang="en"><![CDATA[<p>Is the percentage of children under five years old whose weight for height are less than minus two standards deviations (wasting) and above than plus two standards deviations (overweight) from the median weight for height of the reference population ages 0â€“59 months.</p><p>The nutritional status of children in the survey population was compared with the 2006 WHO Child Growth Standards (WHO, 2006)</p>]]></com:Text>
          
        </gen:ReportedAttribute>
        
        <gen:ReportedAttribute id="UNIT_MEASURE">
          
          <com:Text xml:lang="en"><![CDATA[<p>Percent (%)</p>]]></com:Text>
          
        </gen:ReportedAttribute>
        
        <gen:ReportedAttribute id="FREQ_COLL">
          
          <com:Text xml:lang="en"><![CDATA[<p>3-5 Years</p>]]></com:Text>
          
        </gen:ReportedAttribute>
        
        <gen:ReportedAttribute id="DATA_SOURCE">
          
          <com:Text xml:lang="en"><![CDATA[<p>Rwanda Demographic Health Survey (RDHS)</p>]]></com:Text>
          
        </gen:ReportedAttribute>
        
        <gen:ReportedAttribute id="DATA_COMP">
          
          <com:Text xml:lang="en"><![CDATA[<p>Where C denotes the Number of children under age five that fall below minus two standard deviations from the median weight for height of the 2006 WHO Child Growth Standards and B denotes the total number of children under age five that were measured.</p>]]></com:Text>
          
        </gen:ReportedAttribute>
        
        <gen:ReportedAttribute id="COVERAGE">
          
          <com:Text xml:lang="en"><![CDATA[<h2>Earliest available data</h2><p>1992</p><h2>Geographical coverage</h2><p>National</p><h2>Disaggregation</h2><ul><li>Geographical: National, Province, District &amp; Residence (Urban &amp; Rural)</li><li>Sex</li><li>Age in months</li><li>Socioeconomic status: Marital Status, education level, highest level of degree obtained, wealth quintile</li></ul>]]></com:Text>
          
        </gen:ReportedAttribute>
        
        <gen:ReportedAttribute id="COMPARABILITY">
          
          <com:Text xml:lang="en"><![CDATA[<h2>Link to UN Metadata</h2><p><a href="https://unstats.un.org/sdgs/metadata/files/Metadata-02-02-02a.pdf" target="_blank">United Nations Sustainable Development Goals Metadata (pdf 232kB)</a></p>]]></com:Text>
          
        </gen:ReportedAttribute>
        
        <gen:ReportedAttribute id="OTHER_DOC">
          
          <com:Text xml:lang="en"><![CDATA[<h2>UN designated tier</h2><p>1</p>]]></com:Text>
          
        </gen:ReportedAttribute>
        
      </gen:AttributeSet>
    </gen:Report>
  </mes:MetadataSet>
  
