        <gen:ReportedAttribute id="REC_USE_LIM">
          <com:Text xml:lang="en"/>
        </gen:ReportedAttribute>
        <gen:ReportedAttribute id="DATA_COMP">
          <com:Text xml:lang="en"><![CDATA[<p>The total number of seats occupied by women divided by the total number of seats occupied in parliament and multiplied by 100.</p>]]></com:Text>
        </gen:ReportedAttribute>
